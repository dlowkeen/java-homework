package app;

import pair.BasicPair;
import pair.ArrayPair;
import pair.PairInterface;

public class Main {
	public static void main(String[] args) {
		
		PairInterface<String> myBasicPair = new BasicPair<String>("Kobe", "Shaq");
		System.out.println(myBasicPair.getFirst());
		System.out.println(myBasicPair.getSecond());
		
		PairInterface<Integer> myOtherBasicPair = new BasicPair<Integer>(12, 21);
		System.out.println(myOtherBasicPair.getFirst());
		System.out.println(myOtherBasicPair.getSecond());
		
		int[] myIntArray = new int[]{1, 2};
		PairInterface<Integer> myArrayPair = new ArrayPair<Integer>(myIntArray);
		System.out.println(myArrayPair.getFirst());
		System.out.println(myArrayPair.getSecond());
	}
}
