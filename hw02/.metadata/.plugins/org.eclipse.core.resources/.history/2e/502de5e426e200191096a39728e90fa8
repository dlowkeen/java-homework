package stack;

public class ArrayBoundedStackDriver {
	
	public static void main(String[] args) throws StackOverflowException {
		ArrayBoundedStack<Integer> arrStack = new ArrayBoundedStack<Integer>(5);
		arrStack.push(1);
		System.out.println(arrStack.top());
		arrStack.push(2);
		System.out.println(arrStack.top());
		arrStack.push(3);
		System.out.println(arrStack.top());
		arrStack.push(4);
		System.out.println(arrStack.top());
	}

}
